{
  "name": "berlioz/cli-core",
  "description": "Berlioz Framework CLI Core",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "homepage": "http://getberlioz.com",
  "authors": [
    {
      "name": "Ronan Giron",
      "email": "ronan@getberlioz.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Berlioz\\CliCore\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Berlioz\\CliCore\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "ext-mbstring": "*",
    "berlioz/core": "dev-develop",
    "ulrichsg/getopt-php": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "provide": {
    "psr/container-implementation": "^1.0",
    "psr/log-implementation": "^1.0",
    "psr/simple-cache-implementation": "^1.0"
  },
  "bin": [
    "berlioz"
  ],
  "config": {
    "berlioz": {
      "package": "Berlioz\\CliCore\\CliCorePackage"
    }
  }
}
